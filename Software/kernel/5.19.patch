+static int dump_dma_hdesc(struct hdma_desc * desc, const char * d)
+{
+	int i;
+	unsigned long *p;
+	printk(KERN_DEBUG "%s(): %s\n", __func__, d);
+	p = (unsigned long *)desc;
+	for (i=0;i<8;i++) {
+		printk(KERN_DEBUG "\t%08lx\n", (unsigned long)*p);
+		p++;
+	}
+
+	return 0;
+}
